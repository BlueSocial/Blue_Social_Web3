{
  "compilerOptions": {
    "module": "NodeNext",
    "rootDir": "src",
    "resolveJsonModule": true,
    "allowJs": false,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": false,

    "strict": false,
    "noImplicitAny": false,

    "skipLibCheck": false
  }
}
